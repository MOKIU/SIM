<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mokiu.sim.mapper.RoleMapper">
    <select id="getRoleNameByTeacherId" parameterType="Integer" resultType="String">
        SELECT b.`role_name`
        FROM teacher_role a,
             role b
        WHERE a.`role_id` = b.`role_id`
          AND a.`teacher_id` = #{teacherId}
    </select>
</mapper>